<%# <h1>Skills#new</h1>
<p>Find me in app/views/skills/new.html.erb</p> %>
<h1 class="add-skilltag-title">タグの追加</h1>
<div class="add-skilltag-wrapper">
    <%= form_for @skill do |k| %>
        <div>
            <%= k.label :skill_name,"追加したいスキルのタイトル" %>
            <%= k.text_field :skill_name %>
        </div>
        <div class="add-skilltag-btn">
            <%= k.submit "スキル追加"%>
        </div>
    <% end %>
</div>
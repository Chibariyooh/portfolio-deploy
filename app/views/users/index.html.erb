<%# <h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p> %>
<div class="user">
    <% @user.each do |user| %>
        <div class="user-personal">
            <p class="user-personal-data">
                <span>名前： </span><%= user.name %><br>
                <span>年齢：</span><%= user.age %><br>
                <span>挨拶：</span><%= user.description %>
            </p>
            
            <% user.skills.each do |skill| %>
            <p class="user-personal-skill-name"><%= skill[:skill_name] %></p>
            <!--<p><%= skill[:skill_level] %></p>-->
            <% end %>
            <%# link_to "編集", edit_user_path(user) %>
            <%# link_to "削除", user_path(user), method: :delete, data: {confirm: "削除しますか?"} %>
        </div>
    <% end %>
</div>
